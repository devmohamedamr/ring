load "openssllib.ring"

# md5() sha1() sha256() sha512() .
# md		=	Message Digest .
# sha	=	Secure Hash Algorithm .

? "Enter Your Message : "
give cMsg
? "MD5	= " + md5(cMsg)
? "SHA1	= " + sha1(cMsg)
? "SHA256	= " + sha256(cMsg)











