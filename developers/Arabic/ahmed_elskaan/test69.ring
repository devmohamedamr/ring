new test

new test2 { ? name }

o3 = new test3("ahmed")
o3.print()

new test4("mohamed") {
	print()
}

class test 
	? "welcome"
	# class region
class test2 
	# any variable here is an attribute
	name = "ahmed"
class test3 
	name 
	func init cName 
		name = cName
	func print
		? name
class test4 
	name 
	func init cName 
		name = cName
		return self
	func print
		? name



