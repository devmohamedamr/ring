myString = "str"
myNumber = 5786
myList   = [1,"txt",:A]

? type(myString)
? type(myNumber)
? type(myList)
# also
? isstring(myString)
? isstring(myNumber)
? isstring(myList)
# also
? isnumber(myString)
? isnumber(myNumber)
? isnumber(myList)
# and
? islist(myString)
? islist(myNumber)
? islist(myList)
